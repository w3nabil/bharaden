{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/_vercel/insights/(.*)",
            "dest": "/_vercel/insights/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}